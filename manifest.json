{
  "manifest_version": 3,
  "name": "LeetCodeReminder",
  "description": "Make sure you are reminded of question of the day for leetcode",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/1.png",
      "48": "images/1.png",
      "128": "images/1.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage",
    "background"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  
  "background": {
    "service_worker": "background.js"
  }
}
